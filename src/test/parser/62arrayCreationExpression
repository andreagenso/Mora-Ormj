/* PRUEBAS DE ARRAYCREATION EXPRESSION */

// ArrayCreationExpression -> new PrimitiveType DimExprs Dims opt
// @Anotacion(a = new boolean[34])
// @Anotacion(a = new boolean[34][56][][][])
// @Anotacion(a = new short[23+56][a && b][])
// @Anotacion(a = new byte[23][dsddd])

// ArrayCreationExpression -> new ClassOrInterfaceType DimExprs Dims opt
// @Anotacion(a = new claOIT.sdfd.dfdfdf<ererer.erer>[34])
// @Anotacion(a = new sdfd.sdfsdf[34][56][][][])
// @Anotacion(a = new AAAA<boolean[][][]>[23+56][a && b][])
// @Anotacion(a = new AAAA<boolean[][][],adasd,asdad>[23+56][a && b][])

// ArrayCreationExpression -> new PrimitiveType Dims ArrayInitializer
// @Anotacion(a = new boolean[][][] {})
// @Anotacion(a = new boolean[][]{23,4564+8,aadasd})
// @Anotacion(a = new boolean[][]{23,4564+8,aadasd, {23,2234564564,567567,{}}})

// ArrayCreationExpression -> new ClassOrInterfaceType Dims ArrayInitializer
// @Anotacion(a = new claOIT.sdfd.dfdfdf<ererer.erer>[]{23,2234564564,567567,{}} )
// @Anotacion(a = new sdfd.sdfsdf[][][]{{23,4564+8,aadasd}})
// @Anotacion(a = new AAAA<boolean[][][]>[]{})
@Anotacion(a = new AAAA<boolean[][][],adasd,asdad>[]{qww&&re})

package NombrePaquete;