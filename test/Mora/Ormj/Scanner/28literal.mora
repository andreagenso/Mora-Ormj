/* Pruebas para Literal*/

// Integer Literal
// @Anotacion( dfdf= 0 + 2 * 0372  << 0xDadaCafe > 1996   ? 25 * 5 + 0x00FF00FF : (boolean)34  )
// @Anotacion( dfdf= 9223372036854775808L + 017777777777 * 0x7fffffff  << 0xDadaCafe > 1996   ? 25 * 5 + 0x00FF00FF : (boolean)34  )

// FloatingPointLiteral
// @Anotacion( dfdf=  1e1f*2.f+.3f*0f/3.14f*6.022137e+23f + 2 * 3.4028235e38f  << 0xDadaCafe > 1996   ? 25 * 5 + 0x00FF00FF : (boolean)34  )
// @Anotacion( dfdf= 1e12+.30*.03/.141e-9d-1e137+23f + 1.7976931348623157e308 * 3.4028235e38f  << 0xDadaCafe > 1996   ? 25 * 5 + 0x00FF00FF : (boolean)34  )

/* 0 2 0372 0xDadaCafe 1996 0x00FF00FF    ejemplos de literales enteros
 9223372036854775808L  // Long
 0x7fffffff  // 017777777777 // hexadecimales
 1e1f2.f.3f0f3.14f6.022137e+23f  3.4028235e38f. //ejemplo de float literal
 1e12..30.03.141e-9d1e137      1.7976931348623157e308.   // ejemplo de double literal
*/

// Boolean Literal
// @Anotacion( dfdf= 1e12+.30*.03/.141e-9d-1e137+23f + 1.7976931348623157e308 * 3.4028235e38f  << 0xDadaCafe > true   ? false : (boolean)34  )

// CharacterLiteral
// @Anotacion( dfdf= 'a' + '%'+ '\t'+ '\\'+ '\''+ '\u03a9'+ '\uFFFF'+ '\177' * 3.4028235e38f  << 0xDadaCafe > true   ? false : (boolean)34  )
/* 'a' '%' '\t' '\\' '\'' '\u03a9' '\uFFFF' '\177'  */

// StringLiteral
// @Anotacion( dfdf= "This is a string" + ""+ "\""+ "This is a "+  "two-line string" + '\u03a9'+ '\uFFFF'+ '\177' * 3.4028235e38f  << 0xDadaCafe > true   ? false : (boolean)34  )
/*
""
"\""
"This is a string" // a string containing 16 characters
"This is a " +
 "two-line string"
*/


// NullLiteral
// @Anotacion( dfdf= null)
@Anotacion( dfdf= null + ""+ "\""+ "This is a "+  "two-line string" + '\u03a9'+ '\uFFFF'+ '\177' * 3.4028235e38f  << 0xDadaCafe > true   ? false : (boolean)34  )
